<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Passenger</servlet-name>
        <servlet-class>ammar.sd.programming.control.Passenger</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Driver</servlet-name>
        <servlet-class>ammar.sd.programming.control.Driver</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SmsGate</servlet-name>
        <servlet-class>ammar.sd.programming.control.SmsGate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Passenger</servlet-name>
        <url-pattern>/Passenger</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Driver</servlet-name>
        <url-pattern>/Driver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SmsGate</servlet-name>
        <url-pattern>/SmsGate</url-pattern>
    </servlet-mapping>
</web-app>
